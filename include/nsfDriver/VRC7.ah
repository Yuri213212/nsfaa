}

:FM1_F {
	LADY %FM1OCT STY &VRC7ADDR ST &VRC7DATA
RET }

:FM1_P {
	LI $00 LADY %FM1FREQ STY &VRC7ADDR ST &VRC7DATA
RET }

:FM1_NRV {
	LADY %FM1VOL STY &VRC7ADDR ST &VRC7DATA
