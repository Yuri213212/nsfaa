	LI $00 LADY %FM1OCT STY &VRC7ADDR ST &VRC7DATA
	LD,X &FMNOTE LADY %FM1FREQ STY &VRC7ADDR ST &VRC7DATA
	LD,X &FMOCT LADY %FM1OCT STY &VRC7ADDR ST &VRC7DATA
