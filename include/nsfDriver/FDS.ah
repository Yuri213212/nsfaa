;[W64]
:W64_S {
	LI $C0 ST &W64HIFREQ
RET }

:W64_P {
	LI $00 ST &W64LOFREQ
	LI $40 ST &W64HIFREQ
RET }

:W64_NRV {
	ST &W64VOL
}

:W64_NR {
	LI $C0 ST &W64HIFREQ
	LD,X &W64NOTELO ST &W64LOFREQ
	LD,X &W64NOTEHI ST &W64HIFREQ
RET }

:W64_NV {
	ST &W64VOL
}

:W64_N {
	LD,X &W64NOTELO ST &W64LOFREQ
	LD,X &W64NOTEHI ST &W64HIFREQ
RET }

:W64_V {
	ST &W64VOL
RET }
