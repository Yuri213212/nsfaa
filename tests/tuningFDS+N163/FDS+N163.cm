//@00
var1 fdsnote,n163note;

//@8000
_init_(A){//min 0x19
	if (A<0x19) return;
	Y=A;
	if ((A&=0x0f)>=0x0c) return;
	w64mainhifreq=0xc0;	//w64 key-off
	for (X=0x7f;X>=0;--X){
		n163.X=fxinitdata[X];
	}
	w64modhifreq=0x80;	//disable mod
	w64mainvol=0x8c;	//set vol 0xc(-9dB)
	w64master=0x80;		//wave change begin
	for (X=0x3f;X>=0;--X){
		w64mainwave[X]=w64initdata[X];
	}
	w64master=0x00;		//wave change end
	A=Y;
	if ((A&=0x0f)>=0x0a){
		A+=0x06;
	}else{
		A+=0x02;
	}
	X=A;
	n163note=A<<1&0xe0
	n163note|=X&0x0f;
	A=Y;
	if ((A&=0x0f)>=0x09){
		A=Y-0x19;
	}else{
		A=Y-0x1D;
	}
	fdsnote=X=A;
	w64mainhifreq=w64hifreq[X];
	w64mainlofreq=w64lofreq[X];
	X=n163note|0x10;
	n163.fx3hifreq=0xe4;	//reset phase
	n163.fx3hifreq=fxhifreq[X];
	n163.fx3midfreq=fxmidfreq[X];
	n163.fx3lofreq=fxlofreq[X];
	A=n163note+0x20;
	X=A;
	n163.fx2hifreq=0xe4;	//reset phase
	n163.fx2hifreq=fxhifreq[X];
	n163.fx2midfreq=fxmidfreq[X];
	n163.fx2lofreq=fxlofreq[X];
}

