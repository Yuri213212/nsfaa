		LI $00 ST &REC4HIPERIOD
	;end else if
	LD,Y &FX1INSVOL ANDI $0F
	BZE #10
		ORI $70 LADX %FX1VOL STX &NA ST &ND
	;end if
	LD,Y &FX1INSVOL ANDI $F0
	BZE #23
		ORI $0E TAX
		LD,X &FXHIFREQ ST %FX1TUNING
