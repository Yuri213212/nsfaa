;;;;;;;;;;;;;;;;
;	[code]
;;;;;;;;;;;;;;;;
@9000
:_START_
@9011
:_INIT_ {
	JMP &INIT
}

@9031
:_PLAY_ {
	LD %STATE BNZ #1 RET
	BPZ #11
		LD %COUNT BZE #3
			DEC %COUNT
			RET
		;end if
		LI $01 ST %STATE
	;end if
	LD %COUNT SEC SBC %TEMPOMUL ST %COUNT BCC #1 RET
	ADC %TEMPODIV ST %COUNT
	LDY %ADDR
	LDX,Y &DPCMNOTE
	BNZ #7
		LI $0F ST &ST2A03
	BNZ #35 LD,X &DPCMINST BZE #30	;BNZ for BRA
		LIX $0F STX &ST2A03
		ST &BANKD
		LI $40 ST &DPCMDATA
		LD &SAMPLESPEED ST &DPCMMODE
		LD &SAMPLELENGTH ST &DPCMLEN
		LI $1F ST &ST2A03
	;end else if
	LD,Y &NOISEINSVOL ANDI $0F
	BZE #5
		ORI $30 ST &NOISEVOL
	;end if
	LD,Y &NOISENOTE
	CMPI $FE BNE #17
		LD &ST2A03 ANDI $10 ORI $07 ST &ST2A03
		ORI $0F ST &ST2A03
	BNZ #12 BIT %=$70 BNZ #8	;BNZ for BRA
		ST &NOISEPERIOD
		LI $00 ST &NOISELEN
	;end else if
	LD,Y &TRINOTE TAX
	ANDI $0F CMPI $0C BGE #14
		LD,X &DIVHIPERIOD ST &TRIHIPERIOD
		LD,X &DIVLOPERIOD ST &TRILOPERIOD
	BLO #19 CMPI $0E BNE #15	;BLO for BRA
		LD &ST2A03 ANDI $10 ORI $0B ST &ST2A03
		ORI $0F ST &ST2A03
	;end else if
	LD,Y &REC5INSVOL ANDI $0F
	BZE #7
		ST %REC5VOLBUF
		OR %REC5INSBUF ST &REC5VOL
	;end if
	LD,Y &REC5INSVOL ANDI $F0
	BZE #9
		ORI $30 ST %REC5INSBUF
		OR %REC5VOLBUF ST &REC5VOL
	;end if
	LD,Y &REC5NOTE TAX
	ANDI $0F CMPI $0C BGE #14
		LD,X &DIVHIPERIOD ST &REC5HIPERIOD
		LD,X &DIVLOPERIOD ST &REC5LOPERIOD
	BLO #14 CMPI $0E BNE #10	;BLO for BRA
		LI $02 ST &STMMC5
		LI $03 ST &STMMC5
	;end else if
	LD,Y &REC6INSVOL ANDI $0F
	BZE #7
		ST %REC6VOLBUF
		OR %REC6INSBUF ST &REC6VOL
	;end if
	LD,Y &REC6INSVOL ANDI $F0
	BZE #9
		ORI $30 ST %REC6INSBUF
		OR %REC6VOLBUF ST &REC6VOL
	;end if
	LD,Y &REC6NOTE TAX
